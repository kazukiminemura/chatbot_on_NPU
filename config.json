    "compile_config": {
      "NPU_USE_NPUW": "YES",
      "NPU_COMPILATION_MODE_PARAMS": "compute-layers-with-higher-precision=Softmax,Add",
      "INFERENCE_PRECISION_HINT": "f16"
    }
